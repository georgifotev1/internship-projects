{
  "usesTypeScript": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "./templates/component/TemplateName.tsx"
      }
    },
    "section": {
      "path": "src/sections",
      "withLazy": false,
      "withStory": false,
      "withStyle": true,
      "withTest": true,
      "customTemplates": {
        "component": "./templates/component/TemplateName.tsx"
      }
    }
  },
  "usesCssModule": false
}
